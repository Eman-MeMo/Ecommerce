@{
    ViewData["Title"] = "Index";
    ViewBag.pagename = "Customers";
    ViewBag.img = "admin.jpg";
}
@model List<User>
<partial name="_Banner2" />
<section style="background-color:#e7e6e6;padding:50px;">
    <table class="table" style="border-color:transparent;">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Username</th>
                <th scope="col">Name</th>
                <th scope="col">Age</th>
                <th scope="col">Address</th>
                <th scope="col">Gender</th>
                <th scope="col">Phone</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var u in Model)
            {
                if (u.id != 1)
                {
                    <tr>
                        <th>@u.id</th>
                        <td>@u.username</td>
                        <td>@u.name</td>
                        <td>@u.age</td>
                        <td>@u.address</td>
                        <td>@u.Gender</td>
                        <td>@u.mobileNum</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</section>

